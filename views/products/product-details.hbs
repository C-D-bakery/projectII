<link rel="stylesheet" href="./stylesheets/style.css" />



<h2>Product details</h2>


<section class="book-details">
    <h2>{{title}}</h2>
    <img class="productImgDetails" src={{image}} alt="product"/>
    <p>
        Product: {{product}} <br />
        Name: {{name}} <br />
        Gluten Free: {{gluten_free}}<br/>
        Quantity Available: {{quantity}}<br/>
        {{!-- Made by: {{}} --}}
    </p>
   <ul>
        <strong>Ingredients:</strong>
       
         
            <li>{{this.ingredients}}</li>
        
        
    </ul> 
</section>

<a href="/product">Back</a>
   


